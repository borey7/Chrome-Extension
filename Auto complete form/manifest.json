{
  "manifest_version": 2,
  "name": "Sample name",
  "description": "Sample description",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
      "matches" :  ["*://*/*"],
      "js": ["jQuery-2.8.3.js", "content.js"]
  }],
  "permissions": ["tabs"],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}